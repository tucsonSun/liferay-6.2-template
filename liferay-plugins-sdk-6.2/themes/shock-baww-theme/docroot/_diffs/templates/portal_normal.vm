<!DOCTYPE html>

#parse ($init)

<html class="$root_css_class" dir="#language ("lang.dir")" lang="$w3c_language_id">

	<head>
		<title>$the_title - $company_name</title>
	
		<meta http-equiv="cache-control" content="max-age=0" />
		<meta http-equiv="pragma" content="no-cache" />
		<meta http-equiv="cache-control" content="no-cache, no-store, must-revalidate" />
		<meta http-equiv="expires" content="0" />
		<meta http-equiv="expires" content="Tue, 01 Jan 1979 1:00:00 GMT" />
	    <meta content="initial-scale=1.0, width=device-width" name="viewport" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
	
	     $theme.include($top_head_include)

	     <script src="$javascript_folder/jQuery/2.1.3/jquery-2.1.3.min.js"></script>
	     <script src="$javascript_folder/bootstrap/3.3.2/bootstrap.min.js"></script>
	     <!--<script src="$javascript_folder/jquery-ui-1.10.4.custom.min.js"></script>-->
	     <script src="$javascript_folder/custom.js"></script>
	     <script src="$javascript_folder/main.js"></script>
	</head>
	
	<body class="$css_class">
	
		$theme.include($body_top_include)
			
		<div class="page-wrap">
			<div class="container hidden-tablet hidden-phone visible-desktop">
				<div class="row-flud">
					<div class="span12">
						#if ($is_signed_in && $permissionChecker.isOmniadmin())
						     #dockbar()   
						#end
					</div>
				</div>
			</div>
			
			<div id="content-wrapper">
				<div class="visible-desktop hidden-tablet hidden-phone">
					   #parse ("$full_templates_path/desktop_header.vm")
				</div>
				<div class="container-fluid hidden-desktop visible-tablet visible-phone">
					   #parse ("$full_templates_path/mobile_header.vm")
				</div>
			</div>
		
		
			<div id="content-wrapper">
			     <div class="container" id="content">
			     	 
						#set ($url = $themeDisplay.getURLCurrent())
		     				
						#if($page_group.isSite() && !$layout.getName($themeDisplay.getLocale()).equals("Home"))
							 <nav id="breadcrumbs">#breadcrumbs()</nav>
						#end
		
			          #if ($selectable)
			               $theme.include($content_include)
			          #else
			               $portletDisplay.recycle()
		
			               $portletDisplay.setTitle($the_title)
		
			               $theme.wrapPortlet("portlet.vm", $content_include)
			          #end
			          
			          		<span class="glyphicon glyphicon-comment">c</span>
			     			<span data-unicode="E050" class="glyphicon glyphicon-text-height">t</span>
			     			<h1>test</h1>
			     </div>
			</div>
		
		</div>
		<footer>
			<div id="footer-wrapper">
			     <footer class="container" id="footer" role="contentinfo">
			     	<div class="row-fluid">
			          	<div class="hidden-phone hidden-tablet">
							#parse ("$full_templates_path/desktop_footer.vm")
						</div>
						
						<div id="mobileFooterSocial" class="visible-phone visible-tablet hidden-desktop">
							#parse ("$full_templates_path/mobile_footer.vm")
						</div>
			        </div>
			        
				    <div class="row-fluid">
			        	<div class="span12">
			        		<p>&copy; Shock and Baww 2015</p>
			        	</div>
			        </div>
			              
			     </footer>
			</div>
		</footer>
		
		$theme.include($body_bottom_include)
		
		$theme.include($bottom_include)
	
	
	</body>
</html>